{
  "name": "nean",
  "version": "4.0.0",
  "description": "js framework for neat and lean components",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "watch": "tsdown --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:types": "attw --pack .",
    "coverage": "vitest run --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sovrin/nean.git"
  },
  "author": {
    "name": "Oleg Kamlowski",
    "email": "oleg.kamlowski@thomann.de"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sovrin/nean/issues"
  },
  "homepage": "https://github.com/sovrin/nean#readme",
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.18",
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.10.1",
    "@types/proxyquire": "^1.3.31",
    "@types/react": "^19",
    "@vitest/coverage-istanbul": "^4.0.10",
    "mocha": "^11.7.5",
    "nyc": "^17.1.0",
    "prettier": "^3.6.2",
    "proxyquire": "^2.1.3",
    "react": "^19",
    "ts-node": "^10.9.2",
    "tsdown": "^0.16.5",
    "typescript": "^5.9.3",
    "vitest": "^4.0.10"
  },
  "peerDependencies": {
    "react": ">=16"
  }
}
